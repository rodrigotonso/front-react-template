{
  "name": "new-template-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "_clean": "rimraf .next out build",
    "_control": "run-s lint format type-check test:unit && yarn audit --production && echo \"\nControl finished!\n\"",
    "lint": "eslint --fix --ignore-path .eslintignore \"src/**/*.+(ts|tsx|js|jsx)\" && stylelint --fix --aei --config .stylelintrc \"src/**/*.+(css|scss|sass)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|js|jsx)\"",
    "type-check": "tsc",
    "test:unit": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/unit.html jest --no-cache --forceExit --config jest.config.js --detectOpenHandles ./tests/unit",
    "test:integration": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/integration.html jest --no-cache --forceExit --config jest.config.js --detectOpenHandles ./tests/integration",
    "dev": "next dev",
    "build-stats": "ANALYZE=true run-s build",
    "build": "npm run lint && run-s _clean && next build",
    "export": "npm run build && next export",
    "start": "next start"
  },
  "dependencies": {
    "@date-io/core": "2.13.1",
    "@date-io/date-fns": "2.13.1",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@hookform/resolvers": "2.8.8",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.78",
    "@mui/material": "5.6.2",
    "@reduxjs/toolkit": "1.8.1",
    "axios": "0.26.1",
    "babel-jest": "27.5.1",
    "date-fns": "2.28.0",
    "i18next": "21.6.16",
    "lodash": "4.17.21",
    "next": "12.1.5",
    "next-seo": "5.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "7.30.0",
    "react-icons": "4.3.1",
    "react-redux": "8.0.1",
    "sweetalert2": "11.4.9",
    "ts-auto-mock": "3.5.0",
    "ts-patch": "2.0.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.5",
    "@next/env": "12.1.5",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/react": "18.0.6",
    "@types/testing-library__jest-dom": "5.14.3",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "cross-env": "7.0.3",
    "eslint": "8.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-html-reporter": "3.4.2",
    "jest-ts-auto-mock": "2.1.0",
    "npm-run-all": "4.1.5",
    "postcss-scss": "4.0.4",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sass": "1.50.1",
    "stylelint": "14.7.1",
    "stylelint-config-recommended": "7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.2.0",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "ttypescript": "1.5.13",
    "typescript": "4.6.3"
  }
}
